{
  "expo": {
    "name": "Between Us",
    "slug": "between-us",
    "scheme": "betweenus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1a0614"
    },
    "assetBundlePatterns": [
      "assets/**",
      "content/**"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brittany.betweenus",
      "buildNumber": "1.0.0",
      "deploymentTarget": "17.1",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera access is required to scan your partner’s QR code for secure pairing.",
        "NSPhotoLibraryUsageDescription": "Photo library access lets you add photos to love notes, journal entries, and memories.",
        "ITSAppUsesNonExemptEncryption": true      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": ["C617.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryDiskSpace",
            "NSPrivacyAccessedAPITypeReasons": ["E174.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategorySystemBootTime",
            "NSPrivacyAccessedAPITypeReasons": ["35F9.1"]
          }
        ],
        "NSPrivacyCollectedDataTypes": [
          {
            "NSPrivacyCollectedDataType": "NSPrivacyCollectedDataTypeDeviceID",
            "NSPrivacyCollectedDataTypeLinked": false,
            "NSPrivacyCollectedDataTypeTracking": false,
            "NSPrivacyCollectedDataTypePurposes": ["NSPrivacyCollectedDataTypePurposeAnalytics"]
          },
          {
            "NSPrivacyCollectedDataType": "NSPrivacyCollectedDataTypeCrashData",
            "NSPrivacyCollectedDataTypeLinked": false,
            "NSPrivacyCollectedDataTypeTracking": false,
            "NSPrivacyCollectedDataTypePurposes": ["NSPrivacyCollectedDataTypePurposeAppFunctionality"]
          }
        ]      }
    },
    "android": {
      "package": "com.brittany.betweenus",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1a0614"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "description": "Between Us — Where closeness deepens. A couples app for intimacy, connection, and meaningful moments.",
    "extra": {
      "appName": "Between Us",
      "tagline": "Where closeness deepens.",
      "eas": {
        "projectId": "ab5503ed-b0de-4bf0-aabc-6c46d0c00ec1"
      }
    },
    "experiments": {
      "lazyBundling": false
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-font",
      "@react-native-community/datetimepicker",
      "expo-camera",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#1a0614"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "between-us",
          "organization": "brittany-hornick"
        }
      ],
      ["./plugins/withFullScreenSplash.cjs", {
        "backgroundColor": "#1a0614"
      }]
    ]
  }
}

# RevenueCat API Keys
# Get these from your RevenueCat dashboard: https://app.revenuecat.com/
# Project Settings > API Keys

# iOS API Key (for App Store)
EXPO_PUBLIC_REVENUECAT_IOS_API_KEY=your_ios_api_key_here

# Android API Key (for Google Play)
EXPO_PUBLIC_REVENUECAT_ANDROID_API_KEY=your_android_api_key_here

# Note: For testing, you can use the same test key for both platforms
# Example test key format: test_BkcGFtExPJblUioJmFyCYUSXziS

# Supabase Configuration (for premium users)
# Get these from your Supabase dashboard: Settings â†’ API
EXPO_PUBLIC_SUPABASE_URL="https://xxxxx.supabase.co"
EXPO_PUBLIC_SUPABASE_ANON_KEY="your_anon_key_here"

# App Configuration
EXPO_PUBLIC_APP_ENV=development
